[api]
log_level=CRITICAL
<% if @neutron_user %>
user=<%= @neutron_user %>
<% end %>
<% if @service_token %>
password=<%= @service_token %>
<% end %>
<% if @service_tenant %>
tenant=<%= @service_tenant %>
<% end %>
<% if @auth_url %>
keystone_endpoints= <%= @auth_url %>
<% end %>
nova_os_map=v2/%(tenant_id)s/flavors
glance_map=v1/images
keystone_service_map=v2.0/tenants
keystone_map=v2.0/tenants
cinder_map=v1/%(tenant_id)s/volumes
neutron_map=
contrail_map=
nova_os_timeout=15
glance_timeout=15
keystone_service_timeout=15
keystone_timeout=15
cinder_timeout=15
neutron_timeout=15
