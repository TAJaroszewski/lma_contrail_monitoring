    - name: 'contrail-processes-count-running'
      description: 'Node is running more than 256 processes'
      severity: 'warning'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: processes_count
            fields:
                state: 'running'
            relational_operator: '>='
            threshold: 256
            window: 20
            periods: 0
            function: min

    - name: 'contrail-processes-count-zombies'
      description: 'There are >= 25 zombie processes on node'
      severity: 'warning'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: processes_count
            fields:
                state: 'zombies'
            relational_operator: '>='
            threshold: 25
            window: 20
            periods: 0
            function: min

    - name: 'contrail-processes-count-blocked'
      description: 'There is at least one blocker process on node'
      severity: 'warning'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: processes_count
            fields:
                state: 'blocked'
            relational_operator: '>='
            threshold: 1
            window: 20
            periods: 0
            function: min

    - name: 'system-uptime'
      description: 'The uptime is too low; System was restarted'
      severity: 'warning'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: system-uptime
            relational_operator: '<='
            threshold: 1000
            window: 120
            periods: 0
            function: avg

