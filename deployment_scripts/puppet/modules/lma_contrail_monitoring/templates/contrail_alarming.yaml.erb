---
lma_collector_contrail:
  alarms:
    - name: 'contrail-cpu-critical'
      description: 'The CPU usage is too high (contrail node)'
      severity: 'critical'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: cpu_idle
            relational_operator: '<='
            threshold: 5
            window: 120
            periods: 0
            function: avg

    - name: 'contrail-cpu-warning'
      description: 'The CPU usage is too high (contrail node)'
      severity: 'critical'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: cpu_idle
            relational_operator: '<='
            threshold: 15
            window: 120
            periods: 0
            function: avg

    - name: 'contrail-load-midterm-critical'
      description: 'The load is too high (load_midterm)'
      severity: 'critical'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: load_midterm
            relational_operator: '>='
            threshold: 20
            window: 120
            periods: 0
            function: avg

    - name: 'contrail-load-midterm-warning'
      description: 'The load is too high (load-midterm)'
      severity: 'critical'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: load_midterm
            relational_operator: '>='
            threshold: 5
            window: 120
            periods: 0
            function: avg

    - name: 'contrail-load-shortterm-warning'
      description: 'The load is too high (load-shortterm)'
      severity: 'critical'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
