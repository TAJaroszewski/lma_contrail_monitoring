            relational_operator: '<'
            threshold: 15
            window: 60
            periods: 0
            function: min

    - name: 'contrail-memory-limit-critical'
      description: 'Contrail has reached the memory threshold. All producers are blocked'
      severity: 'critical'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: memory_used
            relational_operator: '<='
            threshold: 1024
            window: 20
            periods: 0
            function: min

    - name: 'contrail-memory-limit-warning'
      description: 'Contrail is getting close to the memory threshold'
      severity: 'warning'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: memory_used
            relational_operator: '<='
            threshold: 1048576000 # 1GB
            window: 20
            periods: 0
            function: min

    - name: 'contrail_db-memory-limit-critical'
      description: 'Contrail has reached the memory threshold. All producers are blocked'
      severity: 'critical'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: memory_used
            relational_operator: '<='
            threshold: 1048576000 # 1GB
            window: 20
            periods: 0
            function: min

    - name: 'contrail_db-memory-limit-warning'
      description: 'Contrail is getting close to the memory threshold'
      severity: 'warning'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: memory_used
            relational_operator: '<='
            threshold: 3048576000 # 3GB
            window: 20
            periods: 0
            function: min

    - name: 'contrail-swap-limit-warning'
      description: 'Contrail node is getting closer to the swap threshold'
      severity: 'warning'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: memory_free
            relational_operator: '<='
            threshold: 3048576000 # 3GB
            window: 20
            periods: 0
            function: min

    - name: 'contrail-swap-limit-critical'
      description: 'Contrail node is getting closer to the swap threshold'
      severity: 'warning'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: memory_free
            relational_operator: '<='
            threshold: 1048576000 # 1GB
            window: 20
            periods: 0
            function: min

    - name: 'contrail-if_errors_rx'
      description: 'One of interfaces is having a log of RX errors'
      severity: 'warning'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
