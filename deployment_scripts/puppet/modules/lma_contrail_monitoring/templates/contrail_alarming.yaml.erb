          - metric: load_shortterm
            relational_operator: '>='
            threshold: 5
            window: 120
            periods: 0
            function: avg

    - name: 'contrail-load-longterm-warning'
      description: 'The load is too high (load-longterm)'
      severity: 'critical'
      enabled: 'true'
      trigger:
        logical_operator: 'or'
        rules:
          - metric: load_longterm
            relational_operator: '>='
            threshold: 5
            window: 120
            periods: 0
            function: avg

    - name: 'root-fs-critical'
      description: "The root filesystem's free space is too low"
      severity: 'critical'
      enabled: 'true'
      trigger:
        rules:
          - metric: fs_space_percent_free
            fields:
              fs: '/'
            relational_operator: '<'
            threshold: 2
            window: 60
            periods: 0
            function: min

    - name: 'root-fs-warning'
      description: "The root filesystem's free space is low"
      severity: 'warning'
      enabled: 'true'
      trigger:
        rules:
          - metric: fs_space_percent_free
            fields:
              fs: '/'
            relational_operator: '<'
            threshold: 5
            window: 60
            periods: 0
            function: min

    - name: 'cassandradb-fs-critical'
      description: "The root filesystem's free space is too low"
      severity: 'critical'
      enabled: 'true'
      trigger:
        rules:
          - metric: fs_space_percent_free
            fields:
              fs: '/var/lib/contrail_db'
            relational_operator: '<'
            threshold: 5
            window: 60
            periods: 0
            function: min

    - name: 'cassandradb-fs-warning'
      description: "The root filesystem's free space is low"
      severity: 'warning'
      enabled: 'true'
      trigger:
        rules:
          - metric: fs_space_percent_free
